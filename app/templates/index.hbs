<div class="row text-left">
  <h1>This is the index page</h1>
</div>

<!-- Search results -->
<p id="search-results" class="container well">

</p>

<!-- List all questions -->
<ul class="list-group">
  {{#each model.questions as |question|}}
    {{#link-to 'question' question.id class='question-tile-link'}}
      {{question-tile question=question}}
    {{/link-to}}
  {{/each}}
</ul>
